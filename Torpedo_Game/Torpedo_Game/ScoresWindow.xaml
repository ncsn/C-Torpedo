<Window x:Class="Torpedo_Game.ScoresWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Torpedo_Game"
        mc:Ignorable="d"
        Title="ScoresWindow" Height="450" Width="800"
        x:Name="scoreWindow">
    <StackPanel>
        <TextBox>Scores</TextBox>
        <Button Content="Back to the MainWindow" x:Name="backButton" Click="backButton_Click" ClickMode="Press"/>
    </StackPanel>
</Window>
