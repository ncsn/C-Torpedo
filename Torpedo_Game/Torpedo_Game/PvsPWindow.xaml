<Window x:Class="Torpedo_Game.PvsPWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Torpedo_Game"
        mc:Ignorable="d"
        Title="PvsPWindow" Height="450" Width="800"
        x:Name="pvspWindow">
    <StackPanel>
        <TextBox x:Name="Player1TextBox">Player1</TextBox>
        <TextBox x:Name="Player2TextBox">Player2</TextBox>
        <Button x:Name="playButton" Click="playButton_Click" ClickMode="Press" Content="Play"/>
        <Button x:Name="backButton" Click="backButton_Click" ClickMode="Press" Content="Back to the MainWindow"/>
    </StackPanel>
</Window>
